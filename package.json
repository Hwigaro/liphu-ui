{
	"name": "@liphu/liphu-ui",
	"version": "0.1.0",
	"author": "Jordhan Saez <jordhan.sm@gmail.com> (http://github.com/hwigaro)",
	"description": "A Vue.js UI-Kit made with â™¥ by Liphu",
	"keywords": [
		"vue",
		"vue2",
		"vuejs",
		"vue.js",
		"library",
		"ui",
		"ui-kit",
		"kit",
		"style",
		"component",
		"components"
	],
	"license": "MIT",
	"main": "dist/liphu-ui.min.js",
	"files": [
		"dist/**/*.*",
		"src/**/*.*"
	],
	"homepage": "https://github.com/LiphuCL/liphu-ui",
	"bugs": {
		"url": "https://github.com/LiphuCL/liphu-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LiphuCL/liphu-ui.git"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=utils,components,lib,theme",
		"build:utils": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=utils",
		"build:components": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=components",
		"build:theme": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=theme",
		"build:lib": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=lib",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js --env.modules=dev",
		"lint:code": "prettier-eslint --write './{site/src,src,test,build}**/*.{js,jsx,vue,json},./site/*.{js,jsx,vue,json}'",
		"lint:style": "prettier-stylelint --write -q './{site/src,src}/**/*.{css,scss,vue}'",
		"lint": "yarn run lint:code && yarn run lint:style",
		"stylelint-check": "stylelint-config-prettier-check",
		"eslint-check": "eslint --print-config .eslint.js | eslint-config-prettier-check",
		"test": "cross-env CI_ENV=/dev/ NODE_ENV=test BABEL_ENV=test karma start test/karma.config.js --single-run",
		"test:watch": "cross-env NODE_ENV=test BABEL_ENV=test karma start test/karma.config.js",
		"version": "auto-changelog -p && git add CHANGELOG.md"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn run lint && yarn run test && yarn run version"
		}
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "changelog.tpl.hbs",
		"unreleased": true,
		"commitLimit": false,
		"replaceText": {}
	},
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.6.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-jsx": "^7.2.0",
		"@babel/plugin-transform-modules-commonjs": "^7.6.0",
		"@babel/plugin-transform-runtime": "^7.6.0",
		"@babel/plugin-transform-spread": "^7.2.2",
		"@babel/preset-env": "^7.6.0",
		"@babel/runtime": "^7.6.0",
		"@vue/test-utils": "^1.0.0-beta.29",
		"auto-changelog": "^1.16.1",
		"babel-eslint": "^10.0.3",
		"babel-helper-vue-jsx-merge-props": "^2.0.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-istanbul": "^5.2.0",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-plugin-syntax-jsx": "^6.18.0",
		"babel-plugin-transform-vue-jsx": "^3.7.0",
		"babel-preset-minify": "^0.5.1",
		"chai": "^4.2.0",
		"cross-env": "^6.0.0",
		"css-loader": "^3.2.0",
		"cssnano": "^4.1.10",
		"eslint": "^6.3.0",
		"eslint-config-prettier": "^6.2.0",
		"eslint-plugin-mocha": "^6.1.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-vue": "^5.2.3",
		"expect": "^24.9.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^4.2.0",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^4.2.5",
		"istanbul-instrumenter-loader": "^3.0.1",
		"jsdom": "^15.1.1",
		"jsdom-global": "^3.0.2",
		"karma": "^4.3.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage": "^2.0.1",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-sinon-chai": "^2.0.2",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^4.0.2",
		"mini-css-extract-plugin": "^0.8.0",
		"mocha": "^6.2.0",
		"node-sass": "^4.12.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0",
		"prettier-eslint-cli": "^5.0.0",
		"prettier-stylelint": "^0.4.2",
		"resolve-url-loader": "^3.1.0",
		"sass-loader": "^8.0.0",
		"sinon": "^7.4.2",
		"sinon-chai": "^3.3.0",
		"stylelint": "^10.1.0",
		"stylelint-config-prettier": "^5.2.0",
		"stylelint-prettier": "^1.1.1",
		"stylelint-scss": "^3.10.1",
		"url-loader": "^2.1.0",
		"vue": "^2.6.11",
		"vue-loader": "^15.7.1",
		"vue-router": "^3.1.3",
		"vue-template-compiler": "^2.6.10",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.8",
		"webpack-dev-server": "^3.8.0",
		"webpack-merge": "^4.2.2",
		"webpack-node-externals": "^1.7.2",
		"webpackbar": "^4.0.0"
	},
	"peerDependencies": {
		"vue": "^2.6.10"
	}
}
